ync function loadReservation() {\n      if (reservation_id) {\n        setError(null);\n        readReservation(reservation_id, abortController.signal)\n          .then(formatReservationDate)\n          .then(formatReservationTime)\n          .then(setFormData)\n          .catch(setError);\n        return () => abortController.abort();\n      }\n    }\n\n    loadReservation();\n  }, [reservation_id]);\n\n  function cancelHandler() {\n    history.goBack();\n  }\n\n  function submitHandler(event) {\n    event.preventDefault();\n    if (reservation_id) {\n      updateReservation(formData, abortController.signal)\n        .then(() => {\n          history.push(`/dashboard/?date=${formData.reservation_date}`);\n        })\n        .catch(setError);\n      return () => abortController.abort();\n    } else {\n      createReservation(formData, abortController.signal)\n        .then(() => {\n          history.push(`/dashboard/?date=${formData.reservation_date}`);\n        })\n        .catch(setError);\n      return () => abortController.abort();\n    }\n  }\n\n  function changeHandler({ target }) {\n    let newValue = target.value;\n    if (target.name === \"people\") {\n      newValue = Number(target.value);\n    }\n    setFormData((previousReservation) => ({\n      ...previousReservation,\n      [target.name]: newValue,\n    }));\n  }\n\n  if (!reservation_id || formData.status === \"booked\") {\n    return (\n      <main>\n        <h1 className=\"mb-3\">\n          {reservation_id ? \"Edit Reservation\" : \"Create Reservation\"}\n        </h1>\n        <form className=\"mb-4\" onSubmit={submitHandler}>\n          <div className=\"row mb-3\">\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"first_name\">\n                First Name\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"first_name\"\n                name=\"first_name\"\n                type=\"text\"\n                value={formData.first_name}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">\n                First name required.\n              </small>\n            </div>\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"last_name\">\n                Last Name\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"last_name\"\n                name=\"last_name\"\n                type=\"text\"\n                value={formData.last_name}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">\n                Last name required.\n              </small>\n            </div>\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"mobile_number\">\n                Mobile Number\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"mobile_number\"\n                name=\"mobile_number\"\n                type=\"tel\"\n                value={formData.mobile_number}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">\n                Mobile number required.\n              </small>\n            </div>\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"reservation_date\">\n                Date of Reservation\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"reservation_date\"\n                name=\"reservation_date\"\n                type=\"date\"\n                placeholder=\"YYYY-MM-DD\"\n                pattern=\"\\d{4}-\\d{2}-\\d{2}\"\n                value={formData.reservation_date}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">Date required.</small>\n            </div>\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"reservation_time\">\n                Time of Reservation\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"reservation_time\"\n                name=\"reservation_time\"\n                type=\"time\"\n                placeholder=\"HH:MM\"\n                pattern=\"[0-9]{2}:[0-9]{2}\"\n                value={formData.reservation_time}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">Time required.</small>\n            </div>\n            <div className=\"col-6 form-group\">\n              <label className=\"form-label\" htmlFor=\"people\">\n                Number in Party\n              </label>\n              <input\n                className=\"form-control\"\n                id=\"people\"\n                name=\"people\"\n                type=\"number\"\n                min=\"1\"\n                value={formData.people}\n                onChange={changeHandler}\n                required={true}\n              />\n              <small className=\"form-text text-muted\">\n                Number in party required.\n              </small>\n            </div>\n          </div>\n\n          <div>\n            <button\n              type=\"button\"\n              className=\"btn btn-secondary mr-2\"\n              onClick={cancelHandler}\n            >\n              Cancel\n            </button>\n            <button type=\"submit\" className=\"btn btn-primary\">\n              Submit\n            </button>\n          </div>\n        </form>\n        <ErrorAlert error={error} />\n      </main>\n    );\n  } else {\n    return <div>Reservation cannot be updated.</div>;\n  }\n}\n","C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\reservations\\ReservationsList.js",[],"C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\tables\\TablesTable.js",[],"C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\reservations\\ReservationTable.js",[],"C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\tables\\TableForm.js",[],"C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\tables\\SeatTable.js",["102"],"import React, { useState } from \"react\";\nimport { seatReservation } from \"../utils/api\";\nimport { useHistory } from \"react-router-dom\";\nimport ErrorAlert from \"../layout/ErrorAlert\";\n\nexport default function SeatTable({ tables = [], reservation_id }) {\n  const history = useHistory();\n  const [tableId, setTableId] = useState(\"\");\n  const [seatedTables, setSeatedTables] = useState([]);\n  const [error, setError] = useState(null);\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    const abortController = new AbortController();\n    setError(null);\n    seatReservation(tableId, reservation_id, abortController.signal)\n      .then((response) => {\n        const newTables = tables.map((table) => {\n          return table.table_id === response.table_id ? response : table;\n        });\n        setSeatedTables(newTables);\n        history.push(\"/dashboard\");\n      })\n      .catch(setError);\n    return () => abortController.abort();\n  }\n\n  // only displaying tables that are not occupied to avoid error\n  const tablesOptions = () => {\n    const availableTables = tables.filter((table) => !table.reservation_id);\n    return availableTables.map((table) => (\n      <option\n        key={table.table_id}\n        value={JSON.stringify(table.table_id)}\n        required={true}\n      >\n        {table.table_name} - {table.capacity}\n      </option>\n    ));\n  };\n\n  return (\n    <>\n      <form>\n        <fieldset>\n          <div className=\"row\"></div>\n          <div className=\"form-group col\">\n            <label htmlFor=\"table_id\">Seat at:</label>\n            <select\n              id=\"table_id\"\n              name=\"table_id\"\n              className=\"form-control\"\n              onChange={(e) => setTableId(e.target.value)}\n            >\n              <option value=\"\">Select a table</option>\n              {tablesOptions()}\n            </select>\n          </div>\n          <button\n            type=\"button\"\n            onClick={history.goBack}\n            className=\"btn btn-secondary mr-2 cancel\"\n          >\n            <span className=\"oi oi-x\"></span> Cancel\n          </button>\n          <button\n            type=\"submit\"\n            onClick={handleSubmit}\n            className=\"btn btn-primary\"\n          >\n            <span className=\"oi oi-check\"></span> Submit\n          </button>\n        </fieldset>\n      </form>\n      <ErrorAlert error={error} />\n    </>\n  );\n}\n","C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\utils\\format-reservation-time.js",[],"C:\\Users\\saman\\OneDrive\\Documents\\FinalCap\\Restaurant-Reservations\\front-end\\src\\tables\\TableList.js",[],{"ruleId":"103","severity":1,"message":"104","line":1,"column":17,"nodeType":"105","messageId":"106","endLine":1,"endColumn":25},{"ruleId":"103","severity":1,"message":"107","line":1,"column":27,"nodeType":"105","messageId":"106","endLine":1,"endColumn":36},{"ruleId":"103","severity":1,"message":"108","line":2,"column":35,"nodeType":"105","messageId":"106","endLine":2,"endColumn":48},{"ruleId":"103","severity":1,"message":"109","line":3,"column":8,"nodeType":"105","messageId":"106","endLine":3,"endColumn":16},{"ruleId":"110","severity":